

<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNG CrossBrowserTesting suite" parallel = "tests" thread-count="3"  >
   
   <test name = "Firefox Browser">  
  <parameter name = "browser" value = "firefox"/>
    <classes>
      <class name="testNG.CrossBrowsertesting"/>         
      
    </classes>
    </test>
    
  <test name="Chrome Browser"  >
  <parameter name = "browser" value = "chrome"/>
    <classes>
      <class name="testNG.CrossBrowsertesting"/>
        </classes>
      </test>
      
     
<test name="Internet Explorer browser"  >
  <parameter name = "browser" value = "ie"/>
    <classes>
      <class name="testNG.CrossBrowsertesting"/>
        </classes>
      </test>
  
</suite> 






  
    




